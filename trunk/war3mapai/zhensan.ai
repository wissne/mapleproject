function ITS takes integer ic returns string
    local string s = ""
    local integer i = ic
    local integer ialt = 0
    local boolean neg = false

    if i == 0 then
      return "0"
    endif
    if i < 0 then
      set neg = true
      set i = -i
    endif
    loop
      exitwhen i == 0
      set ialt = i
      set i = i / 10
      set s = digit_str[ ialt - 10*i ] + s
    endloop
    if neg then
      return "-"+s
    endif
    return s
endfunction












 

