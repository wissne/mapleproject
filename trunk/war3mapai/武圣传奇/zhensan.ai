function AL takes integer ic returns string
    local string s=""
    local integer i=ic
    local integer NL=0
    local boolean BL=false
    if i==0 then
        return"0"
    endif
    if i<0 then
        set BL=true
        set i=-i
    endif
    loop
        exitwhen i==0
        set NL=i
        set i=i/ 10
        set s=AI[NL-10*i]+s
    endloop
    if BL then
        return"-"+s
    endif
    return s
endfunction
