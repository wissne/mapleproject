    local boolean rb=false
    set AM=GetUnitLoc(u2)
    set AP=GP(AM,VN,OQ-130)
    set rb=XQ(u,AP,20)
    call RemoveLocation(AM)
    call RemoveLocation(AP)
    set AP=null
    set AM=null
    return rb
endfunction
